- if admin_signed_in? && current_admin.role == "central"
	.container-fluid.panel-default
		.panel.panel-danger
			.panel-heading
				%h5 Delete User Account
			.panel-body
				%h6 Are you sure you want to delete this account? This action cannot be undone.
				= button_to "Delete User", client_destroy_path(:id => @user.id), :method => :get, data: {confirm: "Are you sure you want to delete your profile? This action cannot be undone."},  class: "hvr-underline-from-left btn btn-md btn-default"
